{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "comma-dangle": [
            0,
            "never"
        ],
        "no-console": 0,
        "indent": [
//            "error",
            2,
            4
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            0,
            "backtick",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}